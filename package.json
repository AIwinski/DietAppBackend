    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/randomstring": "^1.1.6",
    "@types/redis": "^2.8.13",
    "@types/socket.io": "^2.1.3",
    "@types/validator": "^10.11.3",
